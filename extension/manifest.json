{
  "name": "Charcheck",
  "description": "Mark fullwidth numbers and symbols in text content of elements",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
   "default_title": "Click to check characters"
  },
  "permissions": [
    "activeTab", "scripting", "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html"
}
